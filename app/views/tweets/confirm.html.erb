<div class="container">
  <div class="col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1" >
<h3>以下の内容でツイート</h3>
<p class ="content2">
<%= form_for(@tweet) do |f| %>
 <%= f.hidden_field :content %></p>
 <p class ="center2"><%= @tweet.content %></p>
 <p class ="content2">
     <%= f.submit "登録", class: "btn btn-lg btn-primary btn-sm" %>
 </p>
<% end %>
<p class="center">
<%= form_for(@tweet, url: new_tweet_path, method: 'get') do |f| %>
  <%= f.hidden_field :content %>
  <%= f.submit "もどる", name: 'back', class: "btn btn-lg btn-primary btn-sm" %>
<% end %>
</p>
  </div>
</div>